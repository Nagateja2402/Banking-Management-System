<%- include('../partials/header') %>
  </head>
  <body>
      <%- include('../partials/navbar') %>
      <form action="/add/employee" method="post" class="form">
        <h1>Add Employee</h1>
        <div class="info">
            <section>
                <label for="SSN">Enter SSN of Employee:</label>
                <input type="text" id="SSN" placeholder="SSN" name="SSN">
                </section>
                <section>
                <label for="Name">Enter Employee Name:</label>
                <input type="text" name="Name" id="Name" placeholder="Name">
                </section>
                <section>
                    <label for="Email">Enter Employee Email:</label>
                    <input type="text" name="Email" id="Email" placeholder="Email">
                </section>
                <section>
                    <label for="Contact_No">Enter Employee Contact No:</label>
                    <input type="text" name="Contact_No" id="Contact_No" placeholder="Contact_No">
                </section>
                <section>
                    <label for="Address">Enter Employee Address:</label>
                    <input type="text" name="Address" id="Address" placeholder="Address">
                </section>
                <section>
                    <label for="IFSC_Code">Enter Employee IFSC_Code:</label>
                    <input type="text" name="IFSC_Code" id="IFSC_Code" placeholder="IFSC_Code">
                </section>
        </div>
        <button type="submit">Submit</button>
      </form>
    </div>
  </body>
</html>